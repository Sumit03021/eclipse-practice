package Lec5;

public class Post_increment_demo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
        
		int a=7;
		
		int c=++a;
		
		
		System.out.println(c);
		System.out.println(a);
		
		int b= a++ -9;
		
		System.out.println(a);
		System.out.println(b);
		
	}

}
